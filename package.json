{
  "name": "borodutch-club",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.3.1",
    "postcss": "^8.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.0.4",
    "tailwindcss-classnames": "^2.0.7",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "BUILD_PATH='./docs' react-scripts build",
    "build:tailwind": "tailwindcss build src/index.css -o src/tailwind.output.css",
    "generate-css-types": "tailwindcss-classnames -o src/helpers/tailwindClassnames.ts",
    "prestart": "yarn build:tailwind && yarn generate-css-types",
    "prebuild": "yarn build:tailwind && yarn generate-css-types",
    "update": "yarn build && git add . && git commit -m 'update' && git push"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  }
}
